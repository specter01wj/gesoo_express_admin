{{#if_eq status 10}}
    <div class="panel panel-primary orderItems" style="pointer-events:none">
{{else}}
    <div class="panel panel-primary orderItems">
{{/if_eq}}
  <div class="panel-heading orderHead">
    <div class='orderHead_body'>订单号: {{gesooId}}</div>
    <div class='orderHead_body'>合作伙伴: {{partnerCode}}</div>
    {{#if driverName}}
    <div class='orderHead_body'>派送员: {{driverName}}</div>
    {{else}}
    <div class='orderHead_body'>派送员: 暂无</div>
    {{/if}}
    <div class='orderHead_body'>
        {{#if_eq status 10}}
        <div class='orderButton' style="opacity:0.3">
        {{else}}
        <div class='orderButton'>
        {{/if_eq}}
            <a class="assign btn btn-primary" href="#" data-toggle="modal" data-target="#assign_Modal">派送</a>
            <a class="cancel btn btn-danger" href="#">取消</a>
        </div>
    </div>
    <div class='clearfix'></div>
  </div>
  <div class="panel-body">
    <!-- <div class='orderBody'>
        <div class='orderBody_style1'>客户姓名: {{customerName}}</div>
        <div class='orderBody_style1'>客户电话: {{customerPhone}}</div>
